<%= render "shared/error_messages", :target => @user %>

<h1><%= t("editing_user") %></h1>

<%= form_for(:user, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render 'shared/user_form', :f => f %>
  <div class="submit">
    <button type="submit" class="blue">Update Account</button>
  </div>
<% end %>
